# Fingerprint
/dev/fpsdev0                                                                  u:object_r:cdfinger_device:s0
/dev/goodix_fp                                                                u:object_r:fingerprintd_device:s0
/data/system/cdfinger(/.*)?                                                   u:object_r:cdfinger_data_file:s0

/vendor/bin/hw/android\.hardware\.biometrics\.fingerprint@2.1-service\.X00TD                   u:object_r:hal_fingerprint_default_exec:s0

# Hexagon DSP-side executable needed for Halide operation
# This is labeled as public_adsprpcd_file as it needs to be read by apps
# (e.g. Google Camera App)
/mnt/vendor/dsp/fastrpc_shell_3                                               u:object_r:public_adsprpcd_file:s0

# Light HAL
/(vendor|system/vendor)/bin/hw/android\.hardware\.light@2\.0-service\.X00TD                u:object_r:hal_light_default_exec:s0

# Persist
/persist(/.*)?                                                                u:object_r:mnt_vendor_file:s0

# Power
/vendor/bin/hw/android\.hardware\.power-service\.X00TD-libperfmgr        u:object_r:hal_power_default_exec:s0

# Sensors
/sys/devices/virtual/input/input[0-9]+/stk3x[0-9]x-light(/.*)?                    u:object_r:sysfs_sensors:s0
/sys/devices/virtual/input/input[0-9]+/stk3x[0-9]x-proximity(/.*)?                u:object_r:sysfs_sensors:s0

/sys/module/stk3x[0-9]x/parameters/call_status_flag                               u:object_r:sysfs_call_status:s0

# Thermal
/(vendor|system/vendor)/bin/hw/android\.hardware\.thermal@2\.0-service\.qti            u:object_r:hal_thermal_default_exec:s0

# Touch
/(vendor|system/vendor)/bin/hw/vendor\.lineage\.touch@1\.0-service\.X00TD                    u:object_r:hal_lineage_touch_default_exec:s0
